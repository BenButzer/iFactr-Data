setlocal EnableDelayedExpansion
set LINES=0
for /f "usebackq delims==" %25%25I in ("$(ProjectDir)Properties\AssemblyInfo.cs") do (
    set /a LINES=LINES+1    
)
set /a LINES=LINES-3

call:PrintFirstNLine &gt; output.txt

FOR /F "tokens=*" %%A IN ('DATE/T') DO FOR %%B IN (%%A) DO SET Today=%%B
FOR /F "tokens=1-3 delims=/-" %%A IN ("%Today%") DO (
	SET DayMonth=%%A
	SET MonthDay=%%B
	SET Year=%%C
)

echo "%Year%")]&gt;&gt; output.txt
echo [assembly: AssemblyVersion("$(BuildVersion).%BUILDREV%")]&gt;&gt; output.txt
echo [assembly: AssemblyInformationalVersion("$(BuildVersion).%BUILDREV%-%BRANCH%.%COMMITHASH%")]&gt;&gt; output.txt
move output.txt "$(ProjectDir)Properties\AssemblyInfo.cs"

goto EOF

:PrintFirstNLine
set cur=0
for /f "usebackq delims==" %25%25I in ("$(ProjectDir)Properties\AssemblyInfo.cs") do (
    echo %25%25I
    set /a cur=cur+1    
    if "!cur!"=="%25LINES%25" goto EOF
) 

:EOF 

exit /b</PreBuildEvent>
